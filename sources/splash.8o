: splash_draw
	clear
	plane 1
	i := tile_splash_data

	vb := 0
	vc := 32

	loop
		va := 32
		loop
			#:breakpoint blit
			sprite va vb 0
			va += 16
			i += vc
			if va < 96 then
		again
		vb += 16
		if vb < 64 then
	again

	vf := key
	plane 3

	va := 20
	vb := 50
	vc := text_title
	draw_text

	va := 90
	sleep
	clear
	return
