: draw_hline_8
	loop
		sprite va vb 8
		va += 8
		vf := 1
		vc -= vf
		if vc != 0 then
	again
	return

: draw_vline_8
	loop
		sprite va vb 8
		vb += 8
		vf := 1
		vc -= vf
		if vc != 0 then
	again
	return

: draw_tile_32x32
	ve := 64
	sprite va vb 0
	va += 16
	i += ve
	sprite va vb 0
	va += -16
	vb += 16
	i += ve
	sprite va vb 0
	va += 16
	i += ve
	sprite va vb 0
	return
